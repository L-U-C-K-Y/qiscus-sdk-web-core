{
  "name": "qiscus-sdk-core",
  "version": "2.6.6-beta1",
  "description": "Qiscus Web SDK Core",
  "license": "MIT",
  "main": "dist/QiscusSDKCore.min.js",
  "scripts": {
    "build": "webpack --env.production",
    "dev": "webpack --env.development --progress --colors --watch",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "xo",
    "test:unit": "ava"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "^0.25.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-date-fns": "^0.1.0",
    "cross-env": "^5.1.4",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "esm": "^3.0.84",
    "webpack": "^4.22.0",
    "xo": "^0.23.0"
  },
  "dependencies": {
    "date-fns": "^1.28.5",
    "mqtt": "^2.18.1",
    "paho-mqtt": "^1.0.4",
    "r2": "^2.0.0",
    "superagent": "^3.6.3"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "extend": "standard",
    "global": [
      "window",
      "document",
      "FormData",
      "XMLHttpRequest",
      "requestAnimationFrame",
      "vStore"
    ]
  },
  "ava": {
    "require": [
      "@babel/register",
      "esm"
    ]
  }
}

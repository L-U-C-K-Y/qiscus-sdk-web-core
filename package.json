{
  "name": "qiscus-sdk-javascript",
  "version": "3.0.0-alpha.1",
  "description": "Qiscus Web SDK Core",
  "license": "MIT",
  "main": "dist/qiscus-sdk-javascript.cjs.js",
  "browser": "dist/qiscus-sdk-javascript.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:web:prod build:web:dev",
    "build:web:prod": "webpack --env.production --env.target=web",
    "build:web:dev": "webpack --env.development --env.target=web",
    "build:web:watch": "webpack --env.development --env.target=web",
    "clean": "rimraf dist lib",
    "preversion": "npm-run-all clean build",
    "prepublishOnly": "npm-run-all clean build",
    "test": "mocha --require @babel/register --colors 'test/**/*test.js'",
    "test:watch": "npm run test -- -w"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "axios": "^0.19.0",
    "core-js": "^3.2.1",
    "cuid": "^2.1.6",
    "derivable": "^2.0.0-beta.3",
    "es6-symbol": "^3.1.1",
    "fp-ts": "^2.2.0",
    "is_js": "^0.9.0",
    "lodash.flatten": "^4.4.0",
    "mqtt": "^3.0.0",
    "param.macro": "^3.2.1",
    "pietile-eventemitter": "^1.0.0",
    "shades": "^2.1.0",
    "xstream": "^11.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@types/lodash.flatten": "^4.4.6",
    "ava": "^2.3.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-macros": "^2.6.1",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.4",
    "typescript": "^3.7.2",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2"
  }
}

{
  "name": "qiscus-sdk-javascript",
  "version": "3.0.0",
  "description": "Qiscus Web SDK Core",
  "license": "MIT",
  "main": "lib/main.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:node build:web",
    "build:dev": "npm-run-all --parallel build:node build:web:dev",
    "build:node": "tsc",
    "build:node:watch": "tsc --watch",
    "build:web": "webpack --env.production --env.target=web",
    "build:web:dev": "webpack --env.development --env.target=web",
    "build:web:watch": "webpack --env.development --env.target=web",
    "clean": "rimraf dist lib",
    "dev": "webpack --env.development --progress --colors --watch",
    "preversion": "npm-run-all clean build",
    "prepublishOnly": "npm-run-all clean build",
    "test": "mocha --require @babel/register --colors 'test/**/*test.js'",
    "test:watch": "npm run test -- -w",
    "watch": "npm-run-all build:web:watch build:node:watch"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties",
      "babel-plugin-add-module-exports",
      "date-fns"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "auto",
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "cuid": "^2.1.6",
    "date-fns": "^1.28.5",
    "derivable": "^2.0.0-beta.3",
    "es6-symbol": "^3.1.1",
    "is_js": "^0.9.0",
    "ky": "^0.11.1",
    "ky-universal": "^0.2.2",
    "lodash.flatten": "^4.4.0",
    "lodash.map": "^4.6.0",
    "mitt": "^1.1.3",
    "mqtt": "^3.0.0",
    "ow": "^0.13.2",
    "ow-lite": "0.0.2",
    "pietile-eventemitter": "^1.0.0",
    "shades": "^2.1.0",
    "superagent": "^3.6.3",
    "xstream": "^11.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@types/lodash.flatten": "^4.4.6",
    "ava": "^2.3.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-date-fns": "^0.1.0",
    "callbag-take-while": "^2.0.0",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "xo": "^0.24.0"
  }
}
